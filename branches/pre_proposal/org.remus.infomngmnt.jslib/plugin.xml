<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="browserReferenceResource" name="browserReferenceResource" schema="schema/browserReferenceResource.exsd"/>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.remus.infomngmnt.jslib.ImageExtractor"></startup>
   </extension>
   <extension
         point="org.remus.infomngmnt.jslib.browserReferenceResource">
      <referenceResource
            id="org.remus.infomngmnt.jslib.curvycorner"
            resource="js/curvycorners.js">
      </referenceResource>
      <referenceResource
            id="org.remus.infomngmnt.jslib.overlib"
            resource="js/overlib.js">
      </referenceResource>
      <referenceResource
            id="org.remus.infomngmnt.jslib.images"
            resource="templates/images.js">
      </referenceResource>
   </extension>

</plugin>

<?xml version="1.0" encoding="ASCII"?>
<infomngmnt:InformationStructureDefinition xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:infomngmnt="http://remus-software.org/infomngmnt/1.0">
  <structureItems type="STRING" canHaveBinaryReferences="true" label="The path of the images origin" id="originFilePath" createAlways="true"/>
  <structureItems type="LONG" label="The images width" id="width" createAlways="true"/>
  <structureItems type="LONG" label="The images height" id="height" createAlways="true"/>
  <structureItems xsi:type="infomngmnt:DynamicStructure" label="The links or comments within an image" id="links" createAlways="true" upperBound="-1">
    <structureItems label="A single link or comment" id="link">
      <structureItems type="LONG" id="dim_width" createAlways="true"/>
      <structureItems type="LONG" id="dim_height" createAlways="true"/>
      <structureItems type="LONG" id="loc_x" createAlways="true"/>
      <structureItems type="LONG" id="loc_y" createAlways="true"/>
      <structureItems type="STRING" label="Text of the comment" id="text" createAlways="true"/>
    </structureItems>
  </structureItems>
  <structureItems xsi:type="infomngmnt:DynamicStructure" label="Exif Data" id="exifdata" upperBound="-1">
    <structureItems label="Exif Entry" id="exifitem">
      <structureItems type="STRING" label="Exif Key" id="exifkey" createAlways="true"/>
      <structureItems type="STRING" label="Exif Value" id="exifvalue" createAlways="true"/>
    </structureItems>
  </structureItems>
</infomngmnt:InformationStructureDefinition>

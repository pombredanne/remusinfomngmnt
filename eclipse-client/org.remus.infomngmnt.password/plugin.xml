<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.remus.infomngmnt.core.informationType">
      <information
            buildHtml="false"
            creationFactory="org.remus.infomngmnt.password.core.PasswordCreationFactory"
            icon="icons/iconexperience/key.png"
            name="Password"
            presentation="org.remus.infomngmnt.password.AbstractInformationRepresentation"
            type="PASSWORD">
         <validTransfers
               transferId="org.remus.infomngmnt.core.texttransfer">
         </validTransfers>
         <validTransfers
               transferId="org.remus.infomngmnt.core.urlTransfer">
         </validTransfers>
      </information>
   </extension>
   <extension
         point="org.remus.infomngmnt.ui.informationUi">
      <editPage
            editpage="org.remus.infomngmnt.password.EditPasswordPage"
            icon="icons/iconexperience/key.png"
            id="org.remus.infomngmnt.password.editPage1"
            label="Edit Password"
            type="PASSWORD">
      </editPage>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.remus.infomngmnt.ui.simple"
            class="org.remus.infomngmnt.password.NewPasswordWizard"
            finalPerspective="org.remus.infomngmnt.ui.infoperspective"
            icon="icons/iconexperience/password_new.png"
            id="org.remus.infomngmnt.password.newPasswordWizard"
            name="Password">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.remus.infomngmnt.ui.infoperspective">
         <newWizardShortcut
               id="org.remus.infomngmnt.password.newPasswordWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>

</plugin>

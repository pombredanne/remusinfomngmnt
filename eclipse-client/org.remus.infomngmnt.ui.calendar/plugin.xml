<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.ui.editors">
     
      <editor
            class="org.remus.infomngmnt.ui.calendar.CalendarEditor"
            default="false"
            extensions="calendar"
            icon="icons/iconexperience/16/calendar.png"
            id="org.remus.infomngmnt.ui.calendar.CalendarEditor"
            name="%editor.name">
      </editor>
   </extension>
   <extension
         point="org.eclipse.remus.core.postProjectHandle">
      <projectHandle
            handle="org.remus.infomngmnt.ui.calendar.CalendarIndexingHandle">
      </projectHandle>
   </extension>
   <extension
         id="calendarEntryBuilder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.remus.infomngmnt.ui.calendar.CalendarBuilder">
         </run></builder>
   </extension>
   <extension
         point="org.eclipse.remus.ui.editor.informationUi">
      <outlineSection
            class="org.remus.infomngmnt.ui.calendar.outline.CalendarOutlineSection"
            id="org.remus.infomngmnt.ui.calendar.outlineSection">
      </outlineSection>
   </extension>
   <extension
         point="org.eclipse.remus.ui.collapsiblebutton.collapsibleButtonBar">
      <collapsibleButton
            bigImage="icons/iconexperience/24/calendar.png"
            class="org.remus.infomngmnt.ui.calendar.collapsiblebuttons.CalendarSection"
            icon="icons/iconexperience/16/calendar.png"
            id="org.remus.infomngmnt.ui.calendar.collapsibleButton"
            name="%collapsibleButton.name"
            order="15"
            tooltip="%collapsibleButton.tooltip">
      </collapsibleButton>
   </extension>
</plugin>

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.remus.infomngmnt.core.remoteRepository">
      <repository-connector
            class="org.remus.infomngmnt.connector.flickr.FlickrConnector"
            credentialprovider="org.remus.infomngmnt.connector.flickr.FlickrCredentials"
            icon="icons/flickr.png"
            id="org.remus.infomngmnt.connector.flickr."
            name="Flickr">
         <changeDefinition
               id="org.remus.infomngmnt.connector.flickr.changeDefinition1"
               informationType="IMAGE">
            <changeObjectPath
                  path="@keywords">
            </changeObjectPath>
            <changeObjectPath
                  path="@label">
            </changeObjectPath>
            <changeObjectPath
                  path="@description">
            </changeObjectPath>
            <changeObjectPath
                  path="childValues[@type=&apos;originalFilePath&apos;]/@stringValue">
            </changeObjectPath>
            <changeObjectPath
                  path="childValues[@type=&apos;links&apos;]">
            </changeObjectPath>
         </changeDefinition>
      </repository-connector>
   </extension>
   <extension
         point="org.remus.infomngmnt.ui.repositoryUI">
      <respository
            id="org.remus.infomngmnt.connector.flickr.ui"
            newWizard="org.remus.infomngmnt.connector.flickr.ui.NewFlickrRepositoryWizard"
            repository-id="org.remus.infomngmnt.connector.flickr.">
      </respository>
   </extension>

</plugin>

      </index>
      <secondaryIndex
            class="org.remus.infomngmnt.contact.index.AddressAnalyzer"
            node="adress"
            typeId="CONTACT">
      </secondaryIndex>
   </extension>
   <extension
         point="org.eclipse.remus.ui.infotypes.infotypeimages">
      <image
            image="icons/iconexperience/contact.png"
            infotype="CONTACT">
      </image>
   </extension>

</plugin>

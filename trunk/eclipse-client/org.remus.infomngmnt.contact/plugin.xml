<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.remus.core.informationType">
      <information
            buildHtml="true"
            name="%information.name"
            postCreationHandler="org.remus.infomngmnt.contact.core.ContactPostCreation"
            presentation="org.remus.infomngmnt.contact.ContactInformationRepresentation"
            structuredefinition="structuredefinition.xmi"
            type="CONTACT">
      </information>
   </extension>
   <extension
         point="org.eclipse.remus.ui.editor.informationUi">
      <editPage
            editpage="org.remus.infomngmnt.contact.ui.general.EditGeneralPage"
            icon="icons/iconexperience/contact.png"
            id="org.remus.infomngmnt.contact.editGeneralPage"
            label="%general.editPage.label"
            type="CONTACT">
      </editPage>
      <editPage
            editpage="org.remus.infomngmnt.contact.ui.misc.EditMiscPage"
            id="org.remus.infomngmnt.contact.editMiscPage"
            label="%details.editPage.label"
            type="CONTACT">
      </editPage>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.remus.infomngmnt.ui.business"
            class="org.remus.infomngmnt.contact.wizard.NewContactWizard"
            finalPerspective="org.remus.infomngmnt.ui.infoperspective"
            icon="icons/iconexperience/contact_new.png"
            id="org.remus.infomngmnt.contact.newContactWizard"
            name="%wizard.name">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.remus.infomngmnt.ui.infoperspective">
         <newWizardShortcut
               id="org.remus.infomngmnt.contact.newContactWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.remus.ui.infotypes"
            class="org.remus.infomngmnt.contact.ui.preferences.ContactPreferencePage"
            id="org.remus.infomngmnt.contact.preferences"
            name="%page.name">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.remus.infomngmnt.contact.preferences.ContactPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.remus.js.browserReferenceResource">
      <referenceResource
            id="org.remus.infomngmnt.contact.phone"
            resource="icons/iconexperience/phone_call.png">
      </referenceResource>
   </extension>
   <extension
         point="org.eclipse.remus.core.job">
      <job
            class="org.remus.infomngmnt.contact.ui.jobs.CheckBirthdayJob"
            id="org.remus.infomngmnt.contact.birthdayreminder"
            interval="30"
            name="%job.name">
      </job>
   </extension>
   <extension
         point="org.eclipse.remus.ui.rules.ruleDefinition">
      <validTransfers
            transferId="org.eclipse.remus.core.htmltransfer"
            typeId="CONTACT">
      </validTransfers>
      <validTransfers
            transferId="org.eclipse.remus.core.texttransfer"
            typeId="CONTACT">
      </validTransfers>
      <validTransfers
            transferId="org.eclipse.remus.core.urlTransfer"
            typeId="CONTACT">
      </validTransfers>
      <creationtrigger
            class="org.remus.infomngmnt.contact.wizard.NewContactCreationTrigger"
            typeId="CONTACT">
      </creationtrigger>
   </extension>
   <extension
         point="org.eclipse.remus.search.indexer">
      <index
            analyzer="org.remus.infomngmnt.search.SimpleAnalyzer"
            node="title"
            typeId="CONTACT">
      </index>
      <index
            analyzer="org.remus.infomngmnt.search.SimpleAnalyzer"
            node="firstname"
            typeId="CONTACT">
      </index>
      <index
            analyzer="org.remus.infomngmnt.search.SimpleAnalyzer"

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.remus.infomngmnt.core.informationType">
      <information
            buildHtml="true"
            icon="icons/iconexperience/contact.png"
            name="Contact"
            presentation="org.remus.infomngmnt.contact.ContactInformationRepresentation"
            structuredefinition="structuredefinition.xmi"
            type="CONTACT">
         <validTransfers
               transferId="org.remus.infomngmnt.core.texttransfer">
         </validTransfers>
         <validTransfers
               transferId="org.remus.infomngmnt.core.urlTransfer">
         </validTransfers>
         <validTransfers
               transferId="org.remus.infomngmnt.core.rftTransfer">
         </validTransfers>
         <validTransfers
               transferId="org.remus.infomngmnt.core.htmltransfer">
         </validTransfers>
      </information>
   </extension>
   <extension
         point="org.remus.infomngmnt.ui.informationUi">
      <editPage
            editpage="org.remus.infomngmnt.contact.ui.general.EditGeneralPage"
            icon="icons/iconexperience/contact.png"
            id="org.remus.infomngmnt.contact.editGeneralPage"
            label="General"
            type="CONTACT">
      </editPage>
      <editPage
            editpage="org.remus.infomngmnt.contact.ui.misc.EditMiscPage"
            id="org.remus.infomngmnt.contact.editMiscPage"
            label="Details"
            type="CONTACT">
      </editPage>
      <creationUiTrigger
            class="org.remus.infomngmnt.contact.wizard.NewContactWizard"
            typeId="CONTACT">
      </creationUiTrigger>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.remus.infomngmnt.ui.business"
            class="org.remus.infomngmnt.contact.wizard.NewContactWizard"
            finalPerspective="org.remus.infomngmnt.ui.infoperspective"
            icon="icons/iconexperience/contact_new.png"
            id="org.remus.infomngmnt.contact.newContactWizard"
            name="Contact">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.remus.infomngmnt.ui.infoperspective">
         <newWizardShortcut
               id="org.remus.infomngmnt.contact.newContactWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.remus.infomngmnt.ui.infotypes"
            class="org.remus.infomngmnt.contact.ui.preferences.ContactPreferencePage"
            id="org.remus.infomngmnt.contact.preferences"
            name="Contact">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.remus.infomngmnt.contact.preferences.ContactPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.remus.infomngmnt.jslib.browserReferenceResource">
      <referenceResource
            id="org.remus.infomngmnt.contact.phone"
            resource="icons/iconexperience/phone_call.png">
      </referenceResource>
   </extension>
   <extension
         point="org.remus.infomngmnt.core.job">
      <job
            class="org.remus.infomngmnt.contact.ui.jobs.CheckBirthdayJob"
            id="org.remus.infomngmnt.contact.birthdayreminder"
            interval="30"
            name="Birthday-Reminder">
      </job>
   </extension>

</plugin>

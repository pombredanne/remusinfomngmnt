<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.remus.core.informationType">
      <information
            buildHtml="false"
            excludeFromIndex="true"
            name="%information.name"
            presentation="org.remus.infomngmnt.favoritesearch.SearchPresentation"
            structuredefinition="structuredefinition.xmi"
            type="FAVSEARCH">
      </information>
   </extension>
   <extension
         point="org.eclipse.remus.ui.editor.informationUi">
      <editPage
            editpage="org.remus.infomngmnt.favoritesearch.ui.SearchResultPage"
            icon="icons/iconexperience/text_find.png"
            id="org.remus.infomngmnt.favoritesearch.editPage"
            label="%editPage.label"
            type="FAVSEARCH">
      </editPage>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="formheading:org.eclipse.remus.search.ui.view.SearchView?after=additions">
         <command
               commandId="org.remus.infomngmnt.favoritesearch.loadFavSearch"
               icon="icons/iconexperience/fav_search.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.remus.infomngmnt.favoritesearch.handler.LoadFavoriteSearchHandler"
            id="org.remus.infomngmnt.favoritesearch.loadFavSearch"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.remus.ui.infotypes.infotypeimages">
      <image
            image="icons/iconexperience/fav_search.png"
            infotype="FAVSEARCH">
      </image>
   </extension>

</plugin>
